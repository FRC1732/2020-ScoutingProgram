<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <SwitchPreferenceCompat
        app:key="toggle_master"
        app:title="Toggle Master Mode"/>
    <SwitchPreferenceCompat
        app:key="auto_build_json"
        app:title="Automatic Competition JSON Build upon Match Entry">
    </SwitchPreferenceCompat>
    <ListPreference
        app:key="defaultAlliance"
        app:title="Default Alliance"
        app:entries="@array/Alliance"
        app:entryValues="@array/Alliance">
    </ListPreference>
    <EditTextPreference
        app:key="compCode"
        app:title="Competition Code">
    </EditTextPreference>
</PreferenceScreen>